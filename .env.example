# OpenAI API Key - Replace with your actual API key when deploying
LLM_API_KEY=your_openai_api_key_here

# LLM API Endpoint - Default is OpenAI's chat completions endpoint
LLM_API_ENDPOINT=https://api.openai.com/v1/chat/completions

# LLM Model to use - Default is gpt-3.5-turbo
LLM_MODEL=gpt-3.5-turbo

# AgentQL API Key - Required for enhanced web scraping
AGENTQL_API_KEY=your_agentql_api_key_here
AGENTQL_API_URL=https://api.agentql.com/v1/query

# Optional: Customize port for local development
PORT=3000 